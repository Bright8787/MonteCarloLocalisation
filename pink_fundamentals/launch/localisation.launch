<launch>
  <!-- Global parameter: enables or disables PID-based motion -->
  <param name="use_PID" value="false"/>

  <!-- Nodes -->
  <node name="PID_drive" pkg="pink_fundamentals" type="PID_drive" output="screen" />
  <node name="wanderer_try_final" pkg="pink_fundamentals" type="wanderer_try_final" output="screen" />
  <node name="execute_plan_server" pkg="pink_fundamentals" type="execute_plan_server" output="screen" />
  <node name="publish_map_rviz" pkg="pink_fundamentals" type="publish_map_rviz" output="screen" />
  <node name="monte_carlo_matrix" pkg="pink_fundamentals" type="monte_carlo_matrix" output="screen" />
</launch>

